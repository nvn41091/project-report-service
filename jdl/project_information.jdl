entity ProjectInformation {
	id Long
    code String maxlength(200)
    name String maxlength(500)
    startDate Instant
    endDatePlan Instant
    actualEndTime Instant
    money String maxlength(500)
    customerId Long
    companyId Long
    description String maxlength(2000)
    status Long
    updateTime Instant
}
dto ProjectInformation with mapstruct
paginate ProjectInformation with pagination
service ProjectInformation with serviceClass
service ProjectInformation with serviceImpl
